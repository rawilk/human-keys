{
    "name": "rawilk/human-keys",
    "description": "Use Stripe-like IDs for your models.",
    "keywords": [
        "rawilk",
        "laravel",
        "human-keys",
        "laravel-human-keys",
        "stripe",
        "uuid",
        "ulid",
        "ksuid",
        "snowflake",
        "keys"
    ],
    "homepage": "https://github.com/rawilk/human-keys",
    "license": "MIT",
    "authors": [
        {
            "name": "Randall Wilk",
            "email": "randall@randallwilk.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.14"
    },
    "require-dev": {
        "godruoyi/php-snowflake": "^2.2",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ray": "^1.31",
        "tuupola/ksuid": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Rawilk\\HumanKeys\\": "src/",
            "Rawilk\\HumanKeys\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rawilk\\HumanKeys\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rawilk\\HumanKeys\\HumanKeysServiceProvider"
            ],
            "aliases": {
                "HumanKeys": "HumanKeys"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
